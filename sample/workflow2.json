{
    "name": "Simple Workflow",
    "inputs": {
        "a": 100,
        "b": 10
    },
    "steps": {
        "Start": {
            "type": "Start",
            "next": "Write Text File"
        },
        "Write Text File": {
            "type": "Command",
            "command" : "WriteFile",
            "inputs": {
                "text": "\"Hello welcome {a}\"",
                "path": "\"./sdhello.txt\""
            },
            "next": "End"
        },
        "End": {
            "type": "End"
        }
    }
}