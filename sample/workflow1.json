{
    "name": "Simple Workflow",
    "inputs": {
        "a": 100,
        "b": 1000
    },
    "steps": [
        {
            "type": "Start",
            "next": 1
        },
        {
            "type": "Condition",
            "conditions": [
                {
                    "exp": "a>b",
                    "next": 2
                },
                {
                    "exp": "true",
                    "next": 3
                }
            ]
        },
        {
            "type": "Log",
            "message": "\"A is the big number\"",
            "next": 4
        },
        {
            "type": "Log",
            "message": "\"B is the big number\"",
            "next": 4
        },
        {
            "type": "End"
        }
    ]
}